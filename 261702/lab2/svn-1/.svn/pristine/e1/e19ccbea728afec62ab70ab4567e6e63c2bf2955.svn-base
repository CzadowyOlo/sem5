[43.448206, "o", "#!/bin/bash\r\n\r\n# generate list of words in dir. Newline is used as separator\r\nwordlist() {\r\n    find \"$1\" -type f -print0 | xargs -0 cat | sed 's/ \\+/\\n/g' | sort | uniq\r\n}\r\n\r\nwordlist \"$1\" | while read -r word || [[ -n $word ]];\r\ndo\r\n    printf '%s:\\n' \"$word\"\r\n    grep -E \"(^| )$word( |$)\" -rn \"$1\" \r\ndone\r\n"]
